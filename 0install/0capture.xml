<?xml version="1.0" ?>
<?xml-stylesheet type='text/xsl' href='../resources/feed.xsl'?>
<interface xmlns="http://zero-install.sourceforge.net/2004/injector/interface" uri="https://apps.0install.net/0install/0capture.xml">
  <name>0capture</name>
  <summary>captures snapshots of system state and diffs them to generate Zero Install desktop integration</summary>
  <description>0capture is a command-line tool for generating Zero Install feeds with metadata for desktop integration. You can use it to capture the state of your system before installing an app and after. By comparing the two, 0capture can then infer which capabilities the app has and should be represented in its feed.</description>
  <icon href="https://apps.0install.net/0install/0publish.png" type="image/png"/>
  <homepage>https://docs.0install.net/tools/0capture/</homepage>
  <needs-terminal/>

  <group license="LGPL v3 (GNU Lesser General Public License)">
    <command name="run" path="0capture.exe">
      <runner command="run" interface="https://apps.0install.net/dotnet/clr.xml" version="4.7.2.."/>
    </command>

    <implementation version="0.1.0" released="2024-03-29" stability="stable" id="sha256new_5FX6OL5ILDKLZETFHXVVO633NY43A66NB6OYJWGUYTOS3ZPJ3EPQ">
      <manifest-digest sha256new="5FX6OL5ILDKLZETFHXVVO633NY43A66NB6OYJWGUYTOS3ZPJ3EPQ"/>
      <archive href="https://github.com/0install/0capture/releases/download/0.1.0/0capture-0.1.0.tar.zst" size="4757596" type="application/x-zstd-compressed-tar"/>
    </implementation>
    <implementation version="0.1.1" released="2024-07-28" stability="stable" id="sha256new_2BYQ6QTZWUOUTKK43TLNIVCB4PHZBWLFQDS2OAQRYBQAPU7P52NA">
      <manifest-digest sha256new="2BYQ6QTZWUOUTKK43TLNIVCB4PHZBWLFQDS2OAQRYBQAPU7P52NA"/>
      <archive href="https://github.com/0install/0capture/releases/download/0.1.1/0capture-0.1.1.tar.zst" size="4773348" type="application/x-zstd-compressed-tar"/>
    </implementation>
    <implementation version="0.1.2" released="2024-10-28" id="sha256new_PT7RWQIOICHO5E6LVXQGLXAQIOO4QOH2ZSS7LMN2MKJHNCESFSZA">
      <manifest-digest sha256new="PT7RWQIOICHO5E6LVXQGLXAQIOO4QOH2ZSS7LMN2MKJHNCESFSZA"/>
      <archive href="https://github.com/0install/0capture/releases/download/0.1.2/0capture-0.1.2.tar.zst" size="4779962" type="application/x-zstd-compressed-tar"/>
