      <archive href="https://github.com/0install/0install-dotnet/releases/download/2.16.7/0install-dotnet-2.16.7.tar.gz" size="5591098"/>
    </implementation>
  </group>

  <group license="LGPL v3 (GNU Lesser General Public License)">
