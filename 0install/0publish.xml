<interface xmlns="http://zero-install.sourceforge.net/2004/injector/interface" uri="https://apps.0install.net/0install/0publish.xml">
  <name>0publish</name>
  <summary>utility for creating and modifying Zero Install feeds</summary>
  <description>0publish is a command-line tool for creating and modifying Zero Install feed files.</description>
  <homepage>https://docs.0install.net/tools/0publish/</homepage>
  <icon href="https://apps.0install.net/0install/0publish.png" type="image/png"/>
  <needs-terminal/>

  <feed src="https://apps.0install.net/0install/0publish-python.xml" arch="POSIX-*"/>
  <feed src="https://apps.0install.net/0install/0publish-dotnet.xml" arch="Windows-*"/>

  <entry-point binary-name="0publish" command="run"/>
</interface>
