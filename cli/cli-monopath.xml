      <command name="run" path="monopath-emulator.clr2.exe">
        <runner interface="http://repo.roscidus.com/cli/netfx" command="run">
          <version not-before="2.0" before="3.0" />
        </runner>
      </command>
      <command name="run-gui" path="monopath-emulator-win.clr2.exe">
        <runner interface="http://repo.roscidus.com/cli/netfx" command="run-gui">
