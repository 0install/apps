  <icon href="https://raw.githubusercontent.com/0install/apps/master/devel/awka.png" type="image/png"/>
  <category>Development</category>
  <homepage>http://gnuwin32.sourceforge.net/packages/awka.htm</homepage>
  <needs-terminal/>
  <implementation arch="Windows-*" id="sha1new=a2a74bdd007af0d0aed46640693acd85b73c6aec" license="GPL v2 (GNU General Public License)" released="2005-10-27" version="0.7.5-1">
    <command name="run" path="bin/awka.exe"/>
    <manifest-digest sha256new="GYHSRKRGHYX3JQA4QQZHXIOCVVNNEFLSHMO6Y3FSKGJHOVRYIM5Q"/>
    <archive href="https://sourceforge.net/projects/gnuwin32/files/awka/0.7.5-1/awka-0.7.5-1-bin.zip" size="174553" type="application/zip"/>
    <archive href="https://raw.githubusercontent.com/kkeybbs/gnuwin32/blob/master/gnuwin32/awka-bin.zip" size="174553" type="application/zip"/>
  </implementation>
  <package-implementation package="awka" />
  <package-implementation package="dev-util/awka" distributions="Gentoo" />
  <entry-point binary-name="awka" command="run">
    <summary xml:lang="en">AWK language to ANSI C translator and library</summary>
  </entry-point>
</interface>
