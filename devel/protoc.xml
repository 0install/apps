<?xml version="1.0" ?>
<interface uri="https://apps.0install.net/devel/protoc.xml" xmlns="http://zero-install.sourceforge.net/2004/injector/interface">
  <name>protoc</name>
  <summary>compiler/generator for Protocol Buffers (protobuf)</summary>
  <description>Generates source code in various languages (e.g. C++, Java, Python, C#) from protobuf files (.proto).</description>
  <homepage>https://developers.google.com/protocol-buffers/</homepage>
  <needs-terminal/>

  <replaced-by interface="https://apps.0install.net/protobuf/protoc.xml"/>
  <feed src="https://apps.0install.net/protobuf/protoc.xml"/>

  <entry-point binary-name="protoc" command="run"/>
</interface>
