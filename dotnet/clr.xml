      <command name="run">
        <runner interface="http://repo.roscidus.com/dotnet/mono" command="run">
          <version not-before="1.2" before="2.0" />
        </runner>
      </command>
      <command name="run-gui">
        <runner interface="http://repo.roscidus.com/dotnet/mono" command="run-gui">
