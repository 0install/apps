<?xml version="1.0" ?>
<?xml-stylesheet type='text/xsl' href='../resources/feed.xsl'?>
<interface xmlns="http://zero-install.sourceforge.net/2004/injector/interface" uri="https://apps.0install.net/golang/go.xml">
  <name>Go</name>
  <summary>tool for managing Go source code</summary>
  <description>This is a tool for managing Go source code. Go is an open source programming language that makes it easy to build simple, reliable, and efficient software.</description>
  <homepage>https://golang.org/</homepage>
  <needs-terminal/>
  <icon href="https://apps.0install.net/golang/go.png" type="image/png"/>

  <feed src="https://apps.0install.net/golang/go-linux.xml" arch="Linux-*"/>
  <feed src="https://apps.0install.net/golang/go-darwin.xml" arch="Darwin-*"/>
  <feed src="https://apps.0install.net/golang/go-windows.xml" arch="Windows-*"/>

  <entry-point binary-name="go" command="run"/>
  <entry-point binary-name="gofmt" command="gofmt">
    <needs-terminal/>
    <summary>formats Go programs</summary>
    <description>Gofmt formats Go programs. It uses tabs for indentation and blanks for alignment. Alignment assumes that an editor is using a fixed-width font.</description>
  </entry-point>
</interface>
