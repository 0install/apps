<?xml version="1.0" ?>
<?xml-stylesheet type='text/xsl' href='../resources/feed.xsl'?>
<interface xmlns="http://zero-install.sourceforge.net/2004/injector/interface" uri="https://apps.0install.net/gui/gitextensions.xml">
  <name>GitExtensions</name>
  <summary xml:lang="en">GitExtensions is a graphical interface for Git</summary>
  <summary xml:lang="de">GitExtensions ist eine grafische Oberfläche für Git</summary>
  <homepage>https://github.com/gitextensions/gitextensions</homepage>
  <icon href="https://raw.githubusercontent.com/0install/apps/master/gui/gitextensions/gitextensions.ico" type="image/vnd.microsoft.icon"/>
  <icon href="https://raw.githubusercontent.com/0install/apps/master/gui/gitextensions/gitextensions.png" type="image/png"/>
  <category>Development</category>

  <feed src="https://apps.0install.net/gui/gitextensions-2.xml"/>
  <feed src="https://apps.0install.net/gui/gitextensions-3.xml" arch="Windows-*"/>

  <entry-point app-id="GitExtensions" binary-name="GitExtensions" command="run">
    <name>GitExtensions</name>
  </entry-point>
  <entry-point binary-name="gitex" command="gitex">
    <needs-terminal/>
    <name>GitExtensions command-line alias</name>
  </entry-point>

  <capabilities xmlns="http://0install.de/schema/desktop-integration/capabilities">
    <file-type id="GitExtensions">
      <description xml:lang="en">Git Extentions Shortcut</description>
      <verb args="openrepo &quot;%1&quot;" name="open"/>
      <extension value=".gitext"/>
    </file-type>
    <url-protocol id="github-windows">
      <description xml:lang="en">URL: Github for Windows Protocol</description>
      <verb args="%1" name="open"/>
    </url-protocol>
    <url-protocol id="git">
      <description xml:lang="en">URL: Github for Windows Protocol</description>
      <verb args="%1" name="open"/>
    </url-protocol>
    <context-menu id="gitex-browse" target="directories">
      <icon xmlns="http://zero-install.sourceforge.net/2004/injector/interface" href="https://raw.githubusercontent.com/0install/apps/master/gui/gitextensions/browse.ico" type="image/vnd.microsoft.icon"/>
      <verb args="browse &quot;%V&quot;" name="gitex_browse">
        <description>GitExtensions Browse</description>
      </verb>
    </context-menu>
    <context-menu id="gitex-clone" target="directories">
      <icon xmlns="http://zero-install.sourceforge.net/2004/injector/interface" href="https://raw.githubusercontent.com/0install/apps/master/gui/gitextensions/clone.ico" type="image/vnd.microsoft.icon"/>
      <verb args="clone &quot;%V&quot;" name="gitex_clone">
        <description>GitExtensions Clone</description>
      </verb>
    </context-menu>
  </capabilities>
</interface>
