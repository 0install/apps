    <command name="puttygen" path="PUTTYGEN.EXE"/>
    <command name="pageant" path="PAGEANT.EXE"/>
    <command name="plink" path="PLINK.EXE"/>
    <command name="pscp" path="PSCP.EXE"/>
    <command name="psftp" path="PSFTP.EXE"/>

