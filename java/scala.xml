  <group license="BSD License">
    <environment insert="." mode="replace" name="SCALA_HOME"/>
    <environment insert="lib" mode="replace" name="SCALA_LIB"/>
    <command name="run">
      <runner interface="https://apps.0install.net/java/openjdk-jre.xml">
        <arg>-Xmx256M</arg>
        <arg>-Xms32M</arg>
        <arg>-Dscala.home=$SCALA_HOME</arg>
        <arg>-Dscala.usejavacp=true</arg>
        <arg>-cp</arg>
        <arg>$SCALA_LIB/*</arg>
        <arg>scala.tools.nsc.MainGenericRunner</arg>
      </runner>
    </command>
    <command name="scalac">
      <runner interface="https://apps.0install.net/java/openjdk-jre.xml">
        <arg>-Xmx256M</arg>
        <arg>-Xms32M</arg>
        <arg>-Dscala.home=$SCALA_HOME</arg>
        <arg>-Dscala.usejavacp=true</arg>
        <arg>-cp</arg>
        <arg>$SCALA_LIB/*</arg>
        <arg>scala.tools.nsc.Main</arg>
      </runner>
    </command>
    <command name="scaladoc">
      <runner interface="https://apps.0install.net/java/openjdk-jre.xml">
        <arg>-Xmx256M</arg>
        <arg>-Xms32M</arg>
        <arg>-Dscala.home=$SCALA_HOME</arg>
        <arg>-Dscala.usejavacp=true</arg>
        <arg>-cp</arg>
