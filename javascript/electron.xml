<?xml version="1.0" ?>
<?xml-stylesheet type='text/xsl' href='../resources/feed.xsl'?>
<interface xmlns="http://zero-install.sourceforge.net/2004/injector/interface" uri="https://apps.0install.net/javascript/electron.xml">
  <name>Electron</name>
  <summary>framework for creating native applications with web technologies</summary>
  <description>Electron is a framework for creating native applications with web technologies like JavaScript, HTML, and CSS.</description>
  <homepage>https://electronjs.org/</homepage>
  <icon href="https://raw.githubusercontent.com/0install/apps/master/javascript/electron.png" type="image/png"/>
  <icon href="https://raw.githubusercontent.com/0install/apps/master/javascript/electron.icns" type="image/x-icns"/>

  <feed src="https://apps.0install.net/javascript/electron-linux.xml" arch="Linux-*"/>
  <feed src="https://apps.0install.net/javascript/electron-macos.xml" arch="MacOSX-*"/>
  <feed src="https://apps.0install.net/javascript/electron-windows.xml" arch="Windows-*"/>
  <package-implementation distributions="Virtual" package="electron" main="/usr/bin/electron"/><!-- Prevent this virtual feed from being seen as a library feed -->

  <entry-point binary-name="electron" command="run"/>
</interface>
