<?xml version="1.0" encoding="utf-8"?>
<interface xmlns="http://zero-install.sourceforge.net/2004/injector/interface">
  <name>NW.js SDK</name>
  <summary>NW.js with DevTools for debugging</summary>
  <description>This is the full NW.js plus additional DevTools for debugging.</description>
  <homepage>https://nwjs.io/</homepage>
  <icon href="http://nwjs.io/img/logo.png" type="image/png"/>

  <feed-for interface="https://apps.0install.net/javascript/nwjs-sdk.xml"/>

  <group license="MIT License">
    <implementation arch="Linux-x86_64" main="nw" version="{version}" released="{released}" stability="{stability}">
      <manifest-digest/>
      <archive href="https://dl.nwjs.io/v{version-original}/nwjs-sdk-v{version-original}-linux-x64.tar.gz" type="application/x-compressed-tar" extract="nwjs-sdk-v{version-original}-linux-x64"/>
    </implementation>
    <implementation arch="Linux-i486" main="nw" version="{version}" released="{released}" stability="{stability}">
      <manifest-digest/>
      <archive href="https://dl.nwjs.io/v{version-original}/nwjs-sdk-v{version-original}-linux-ia32.tar.gz" type="application/x-compressed-tar" extract="nwjs-sdk-v{version-original}-linux-ia32"/>
    </implementation>
    <implementation arch="Darwin-x86_64" main="nwjs.app/Contents/MacOS/nwjs" version="{version}" released="{released}" stability="{stability}">
      <manifest-digest/>
      <archive href="https://dl.nwjs.io/v{version-original}/nwjs-sdk-v{version-original}-osx-x64.zip" type="application/zip" extract="nwjs-sdk-v{version-original}-osx-x64"/>
    </implementation>
    <implementation arch="Windows-x86_64" main="nw.exe" version="{version}" released="{released}" stability="{stability}">
      <manifest-digest/>
      <archive href="https://dl.nwjs.io/v{version-original}/nwjs-sdk-v{version-original}-win-x64.zip" type="application/zip" extract="nwjs-sdk-v{version-original}-win-x64"/>
    </implementation>
    <implementation arch="Windows-i486" main="nw.exe" version="{version}" released="{released}" stability="{stability}">
      <manifest-digest/>
      <archive href="https://dl.nwjs.io/v{version-original}/nwjs-sdk-v{version-original}-win-ia32.zip" type="application/zip" extract="nwjs-sdk-v{version-original}-win-ia32"/>
    </implementation>
  </group>
</interface>
