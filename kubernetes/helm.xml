  <needs-terminal/>

  <feed src="https://apps.0install.net/kubernetes/helm-2.xml"/>
  <feed src="https://apps.0install.net/kubernetes/helm-3.xml"/>

  <entry-point binary-name="helm" command="run"/>
</interface>
