	<archive href="https://github.com/0install/apps.0install.net/releases/download/archives/libgtkmm-2.4-1c2a-i386-1_2.20.3-1.tar.bz2" size="1276223"/>
      </implementation>
    </group>
    <group arch="Linux-x86_64">
      <implementation id="sha1new=bb479194c2204b6cea6ada917fc46725e9c0afbf" released="2012-01-01" version="2.20.3-1">
