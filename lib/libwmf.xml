<?xml version="1.0" ?>
<?xml-stylesheet type='text/xsl' href='interface.xsl'?>
<interface uri="https://apps.0install.net/lib/libwmf.xml" xmlns="http://zero-install.sourceforge.net/2004/injector/interface" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://zero-install.sourceforge.net/2004/injector/interface http://0install.de/schema/injector/interface/interface.xsd http://0install.de/schema/desktop-integration/capabilities http://0install.de/schema/desktop-integration/capabilities/capabilities.xsd">
  <name>LibWmf</name>
  <summary xml:lang="en">LibWmf: Windows Metafile Graphics library</summary>
  <description xml:lang="en">libwmf is a library for reading vector images in Microsøft's native Windøws Metafile Format (WMF) and for either (a) displaying them in, e.g., an X window; or (b) converting them to more standard/open file formats such as, e.g., the W3C's XML-based Scaleable Vector Graphic (SVG) format. Currently bindings exist for conversion to: 
•the following vector image formats: ◦(&amp; Encapsulated) PostScript (EPS &amp; PS) 
◦Facility for Interactive Generation of graphics (FIG) 
◦Scaleable Vector Graphic (SVG) 

•the following raster image formats: ◦Portable Network Graphics (PNG) 
◦Joint Photographic Experts Group (JPEG) 

</description>
  <icon href="https://raw.githubusercontent.com/0install/apps/master/lib/libwmf.ico" type="image/vnd.microsoft.icon"/>
  <icon href="https://raw.githubusercontent.com/0install/apps/master/lib/libwmf.png" type="image/png"/>
  <category>Development</category>
  <homepage>http://gnuwin32.sourceforge.net/packages/libwmf.htm</homepage>
  <needs-terminal/>
  <implementation arch="Windows-i486" id="sha1new=761f4f51f668431bb5cd78316d8e65161c320d24" license="GPL v2 (GNU General Public License)" released="2005-01-18" version="0.2.8.3-1-3">
    <requires interface="https://apps.0install.net/devel/freetype.xml" version="2..!3">
      <environment insert="bin" name="PATH"/>
    </requires>
    <requires interface="https://apps.0install.net/lib/jpeg.xml">
      <environment insert="bin" name="PATH"/>
    </requires>
    <requires interface="https://apps.0install.net/lib/png12-0.xml">
      <environment insert="bin" name="PATH"/>
    </requires>
    <requires interface="https://apps.0install.net/utils/plotutils.xml">
      <environment insert="bin" name="PATH"/>
    </requires>
    <requires interface="https://apps.0install.net/lib/zlib.xml">
      <environment insert="bin" name="PATH"/>
    </requires>
    <requires interface="https://apps.0install.net/lib/xml2.xml">
      <environment insert="bin" name="PATH"/>
    </requires>
    <command name="run" path="bin/wmf2svg.exe"/>
    <command name="libwmf-fontmap" path="bin/libwmf-fontmap">
      <requires interface="https://apps.0install.net/utils/sed.xml">
        <executable-in-path name="sed"/>
      </requires>
      <requires interface="https://apps.0install.net/utils/grep.xml">
        <executable-in-path name="grep"/>
      </requires>
      <requires interface="https://apps.0install.net/utils/coreutils.xml">
        <executable-in-path command="cat" name="cat"/>
        <executable-in-path command="ls" name="ls"/>
      </requires>
      <runner interface="https://apps.0install.net/utils/bash.xml"/>
    </command>
    <command name="wmf2eps" path="bin/wmf2eps.exe"/>
    <command name="wmf2fig" path="bin/wmf2fig.exe"/>
    <command name="wmf2gd" path="bin/wmf2gd.exe"/>
    <command name="wmf2plot" path="bin/wmf2plot.exe"/>
    <manifest-digest sha256new="64A5QPMTOLSVUJXAL7E2RZKARTWVSOK3PVQERNXJ7Y3B4UVS37VQ"/>
    <archive href="https://sourceforge.net/projects/gnuwin32/files/libwmf/0.2.8.3-1/libwmf-0.2.8.3-1-bin.zip" size="944542" type="application/zip"/>
    <archive href="https://github.com/kkeybbs/gnuwin32/blob/master/gnuwin32/libwmf-bin.zip?raw=true" size="944542" type="application/zip"/>
  </implementation>
  <package-implementation distributions="Gentoo" package="media-libs/libwmf"/>
  <package-implementation package="libwmf"/>
  <entry-point binary-name="wmf2svg" command="run">
    <needs-terminal/>
    <name xml:lang="en">wmf2svg</name>
    <summary xml:lang="en">Convert metafile image to W3C's scaleable vector graphic (SVG) format.</summary>
  </entry-point>
  <entry-point binary-name="libwmf-fontmap" command="libwmf-fontmap">
    <needs-terminal/>
    <name xml:lang="en">libwmf-fontmap</name>
  </entry-point>
  <entry-point binary-name="wmf2eps" command="wmf2eps">
    <needs-terminal/>
    <name xml:lang="en">wmf2eps</name>
    <summary xml:lang="en">Convert metafile image to postscript.</summary>
  </entry-point>
  <entry-point binary-name="wmf2fig" command="wmf2fig">
    <needs-terminal/>
    <name xml:lang="en">wmf2fig</name>
    <summary xml:lang="en">Convert metafile image to XFig format.</summary>
  </entry-point>
  <entry-point binary-name="wmf2gd" command="wmf2gd">
    <needs-terminal/>
    <name xml:lang="en">wmf2gd</name>
    <summary xml:lang="en">Convert metafile image to one of: png,jpg.</summary>
  </entry-point>
  <entry-point binary-name="wmf2plot" command="wmf2plot">
    <needs-terminal/>
    <name xml:lang="en">wmf2plot</name>
    <summary xml:lang="en">Convert metafile image to one of: png,jpg.</summary>
    <description xml:lang="en">generic,bitmap,meta,tek,regis,hpgl,pcl,fig,cgm,ps,ai,svg,gif,pnm,png,X</description>
  </entry-point>
</interface>
