    </requires>
      <implementation id="sha1new=8390965d5ff42850c411b6902a72b5f7683ae1ae" released="2012-03-27" version="1.2.44-1">
      <archive href="libpng12-0-i386-1.2.44-1+squeeze3.tar.bz2" size="106547"/>
