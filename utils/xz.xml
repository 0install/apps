  <entry-point binary-name="xz" command="run">
    <name>compress .xz file</name>
    <needs-terminal/>
  </entry-point>
  <entry-point command="unxz">
    <name>decompress .xz file</name>
    <needs-terminal/>
  </entry-point>
  <entry-point command="xzcat">
    <name>decompress .xz file to stdout</name>
    <needs-terminal/>
  </entry-point>
  <entry-point command="lzma">
    <name>compress .lzma file</name>
    <needs-terminal/>
  </entry-point>
  <entry-point command="unlzma">
    <name>decompress .lzma file</name>
    <needs-terminal/>
  </entry-point>
  <entry-point command="lzcat">
    <name>decompress .lzma file to stdout</name>
    <needs-terminal/>
  </entry-point>
</interface>
