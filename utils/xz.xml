<?xml version="1.0" ?>
<?xml-stylesheet type='text/xsl' href='../resources/feed.xsl'?>
<interface uri="https://apps.0install.net/utils/xz.xml" xmlns="http://zero-install.sourceforge.net/2004/injector/interface">
  <name>XZ Utils</name>
  <summary>xz data compression tool for .xz and .lzma files</summary>
  <description>XZ Utils is free general-purpose data compression software with high compression ratio. XZ Utils are the successor to LZMA Utils.</description>
  <homepage>http://tukaani.org/xz/</homepage>
  <category>Utility</category>
  <needs-terminal/>

  <package-implementation distributions="Debian" main="/usr/bin/gnuplot" package="xz-utils">
    <command name="unxz" path="/usr/bin/unxz"/>
    <command name="xzcat" path="/usr/bin/xzcat"/>
    <command name="lzma" path="/usr/bin/lzma"/>
    <command name="unlzma" path="/usr/bin/unlzma"/>
    <command name="lzcat" path="/usr/bin/lzcat"/>
  </package-implementation>
  <package-implementation distributions="RPM" main="/usr/bin/gnuplot" package="xz">
    <command name="unxz" path="/usr/bin/unxz"/>
    <command name="xzcat" path="/usr/bin/xzcat"/>
    <command name="lzma" path="/usr/bin/lzma"/>
    <command name="unlzma" path="/usr/bin/unlzma"/>
    <command name="lzcat" path="/usr/bin/lzcat"/>
  </package-implementation>

  <group license="LGPL (GNU Lesser General Public License)">
    <command name="run" path="xz.exe"/>
    <command name="unxz" path="xz.exe">
      <arg>--decompress</arg>
    </command>
    <command name="xzcat" path="xz.exe">
      <arg>--decompress</arg>
      <arg>--stdout</arg>
    </command>
    <command name="lzma" path="xz.exe">
      <arg>--format=lzma</arg>
    </command>
    <command name="unlzma" path="xz.exe">
      <arg>--format=lzma</arg>
      <arg>--decompress</arg>
    </command>
    <command name="lzcat" path="xz.exe">
      <arg>--format=lzma</arg>
      <arg>--decompress</arg>
      <arg>--stdout</arg>
    </command>
