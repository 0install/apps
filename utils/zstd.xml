<?xml version="1.0" ?>
<interface xmlns="http://zero-install.sourceforge.net/2004/injector/interface" uri="https://apps.0install.net/utils/zstd.xml">
  <name>Zstandard</name>
  <summary>fast real-time compression algorithm</summary>
  <description>Zstandard is a fast compression algorithm, providing high compression ratios. It also offers a special mode for small data, called dictionary compression.</description>
  <category>Utility</category>
  <homepage>https://facebook.github.io/zstd/</homepage>
  <needs-terminal/>

  <package-implementation distributions="RPM Debian" main="/usr/bin/zstd" package="zstd">
    <command name="multi-threaded" path="/usr/bin/zstdmt"/>
    <command name="decompress" path="/usr/bin/unzstd"/>
    <command name="cat" path="/usr/bin/zstdcat"/>
  </package-implementation>

